<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Press+Start+2P">
    <title>Pokeagenda</title>
</head>
<body>
    <header>
    
    </header>
        <main>
            <section>
                <img id="pok" src="/img/pokemon.png">    
            </section>
          <section id="pokedex">
            <section class="sensor">
              <button id="sensor"></button>
            </section>
            <section class="camera-display">
                <img id="pokemon" src="img/charmander.jpg">
            </section>
            <section class="sectionider"></section>
            <section class="stats-display">
              <h2 id="name">Charmander</h2>
            <section id="n">
              <h5>Num: </h2><div id="num"> 001 </div>
            </section>
            <h3>Descrição</h3>
                <li>TIPO:</li><p>Fogo </p>
              </ul>
            </section>
            <section class="botom-actions">
              <section id="actions">
                <button class="a"><a href="./details">Select</a></button>
              </section>
              <section id="cross">
                <button class="cross-button up"></button>
                <button class="cross-button right"></button>
                <button class="cross-button down"></button>
                <button class="cross-button left"></button>
                <section class="cross-button center"> </section>
              </section>
            </section>
            <!-- <section class="input-pad"><input /></section> -->
          
            <section class="bottom-modes">
              
                <button class="level-button">FOTO</button>
                <button class="level-button">EVO 1</button>
                <button class="level-button">EVO 2</button>
                <button class="level-button">EVO 3</button>
              
                <button class="pokedex-mode black-button"><a href="./index">HOME</a></button>
                <button class="game-mode black-button"><a href="./cadastro">CADASTRAR</a></button>
              
            </section>
          
          </section>
        </main>
    <!-- <footer>
        &copy; Todos os direitos reservados a mim.
    </footer> -->
</body>
</html>